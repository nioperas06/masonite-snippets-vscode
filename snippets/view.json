{
    "view": {
		"scope": "python",
		"prefix": "view",
        "body": [
            "view('${1:name}')"
        ],
        "description": "view"
    },
    "view with data": {
		"scope": "python",
		"prefix": "view.param",
        "body": [
            "view('${1:name}', {'${2:param}': ${3:value}})"
        ],
        "description": "view with data"
    },
    "view sharing": {
		"scope": "python",
		"prefix": "view.param",
        "body": [
            "view.share({'${1:param}': ${2:value}})"
        ],
        "description": "view with data"
    },
    "view composer": {
		"scope": "python",
		"prefix": "view.param",
        "body": [
            "view.composer('${1:name}', {'${2:param}': ${3:value}})"
        ],
        "description": "view with data"
    }
}