{
    "GET Route": {
		"scope": "python",
		"prefix": "Get",
        "body": [
            "Get('/${1:url/here}', '${2:Controller}@${3:method}'),"
        ],
        "description": "GET Route"
	},
	"GET named Route": {
		"scope": "python",
		"prefix": "Get.name",
        "body": [
            "Get('/${1:url/here}', '${2:Controller}@${3:method}').name('${4:name}'),"
        ],
        "description": "GET named Route"
	},
	"GET Route with middleware": {
		"scope": "python",
		"prefix": "Get.middleware",
        "body": [
            "Get('/${1:url/here}', '${2:Controller}@${3:method}').middleware('${4:name}'),"
        ],
        "description": "Basic Get Route with shorthand get."
	},
	"GET named with middleware - 1": {
		"scope": "python",
		"prefix": "Get.middleware.name",
        "body": [
            "Get('/${1:url/here}', '${2:Controller}@${3:method}').middleware('${4:name}').name('${5:name}'),"
        ],
        "description": "GET named with middleware - 1"
    },
    "GET named with middleware - 2": {
		"scope": "python",
		"prefix": "Get.name.middleware",
        "body": [
            "Get('/${1:url/here}', '${2:Controller}@${3:method}').name('${4:name}').middleware('${5:name}'),"
        ],
        "description": "GET named with middleware - 2"
	}
}