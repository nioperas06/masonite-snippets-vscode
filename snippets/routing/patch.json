{
    "PATCH Route": {
		"scope": "python",
		"prefix": "Patch",
        "body": [
            "Patch('/${1:url/here}', '${2:Controller}@${3:method}'),"
        ],
        "description": "PATCH Route"
	},
	"PATCH named Route": {
		"scope": "python",
		"prefix": "Patch.name",
        "body": [
            "Patch('/${1:url/here}', '${2:Controller}@${3:method}').name('${4:name}'),"
        ],
        "description": "PATCH named Route"
	},
	"PATCH Route with middleware": {
		"scope": "python",
		"prefix": "Patch.middleware",
        "body": [
            "Patch('/${1:url/here}', '${2:Controller}@${3:method}').middleware('${4:name}'),"
        ],
        "description": "PATCH Route with middleware"
	},
	"PATCH named Route with middleware - 1": {
		"scope": "python",
		"prefix": "Patch.middleware.name",
        "body": [
            "Patch('/${1:url/here}', '${2:Controller}@${3:method}').middleware('${4:name}').name('${5:name}'),"
        ],
        "description": "PATCH named Route with middleware - 1"
    },
    "PATCH named Route with middleware - 2": {
		"scope": "python",
		"prefix": "Patch.name.middleware",
        "body": [
            "Patch('/${1:url/here}', '${2:Controller}@${3:method}').name('${4:name}').middleware('${5:name}'),"
        ],
        "description": "PATCH named Route with middleware - 2"
	}
}