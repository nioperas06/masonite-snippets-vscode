{
    "auth login": {
		"scope": "python",
		"prefix": "auth.login",
        "body": [
            "auth.login('${1:user@email.com}', '${2:password}')"
        ],
        "description": "auth login"
    },
    "auth user": {
		"scope": "python",
		"prefix": "auth.user",
        "body": [
            "auth.user()"
        ],
        "description": "auth user"
    },
    "request user": {
		"scope": "python",
		"prefix": "request.user",
        "body": [
            "request.user()"
        ],
        "description": "request.user"
    },
    "login once": {
		"scope": "python",
		"prefix": "auth.once.login",
        "body": [
            "auth.once().login('${1:user@email.com}', '${2:password}')"
        ],
        "description": "auth.once.login"
    },
    "register user": {
		"scope": "python",
		"prefix": "auth.register",
        "body": [
            "auth.register('${1:user@email.com}', '${2:password}', '${3:name}')"
        ],
        "description": "auth.register"
    },
    "auth logout": {
		"scope": "python",
		"prefix": "auth.logout",
        "body": [
            "auth.logout()"
        ],
        "description": "Auth logout"
    }
}