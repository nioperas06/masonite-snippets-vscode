{
    "Auth login": {
		"scope": "python",
		"prefix": "Auth().login",
        "body": [
            "Auth(${1:Request}).login('${2:user@email.com}', '${3:password}')"
        ],
        "description": "Auth login"
    },
    "Auth user": {
		"scope": "python",
		"prefix": "Auth().user",
        "body": [
            "Auth(${1:Request}).user()"
        ],
        "description": "Auth user"
    },
    "Auth logout": {
		"scope": "python",
		"prefix": "Auth().user",
        "body": [
            "Auth(${1:Request}).logout()"
        ],
        "description": "Auth logout"
    }
}